{
  "name": "@repo/constants",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc --build",
    "build:clean": "tsc --build --clean",
    "build:force": "tsc --build --force",
    "build:rebuild": "npm run build:clean && npm run build:force",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit --skipLibCheck",
    "check-types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "eslint": "^9.29.0",
    "tsup": "^8.3.5",
    "typescript": "5.8.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  }
}
