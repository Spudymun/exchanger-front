{
  "name": "admin-panel",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "lint:strict": "next lint --max-warnings 0",
    "lint:styles": "stylelint \"app/**/*.{css,scss}\" --fix",
    "check-types": "tsc --noEmit",
    "db:reset": "npx prisma db execute --file ../../packages/session-management/scripts/reset/admin-reset.sql --schema=../../packages/session-management/prisma/schema.prisma",
    "db:reset:shared": "npx prisma db execute --file ../../packages/session-management/scripts/reset/shared-reset.sql --schema=../../packages/session-management/prisma/schema.prisma",
    "db:studio": "npx prisma studio --schema=../../packages/session-management/prisma/schema.prisma"
  },
  "dependencies": {
    "@repo/providers": "*",
    "@repo/ui": "*",
    "next": "^15.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "eslint": "^9.29.0",
    "eslint-config-next": "^15.3.0",
    "typescript": "5.8.2"
  }
}
